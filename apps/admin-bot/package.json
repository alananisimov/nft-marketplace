{
  "name": "@acme/admin-bot",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "clean": "git clean -xdf .cache .expo .turbo android ios node_modules",
    "dev": "pnpm with-env tsx --tsconfig tsconfig.json --watch ./src/index.ts",
    "start": "pnpm with-env tsx --tsconfig tsconfig.json ./src/index.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "@acme/api": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@telegraf/session": "2.0.0-beta.7",
    "@trpc/client": "^11.0.0-rc.477",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "superjson": "2.2.1",
    "telegraf": "^4.16.3",
    "telegraf-session-local": "^2.1.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@acme/api": "workspace:*",
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}
