{
  "name": "@acme/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./trpc": "./src/trpc.ts",
    "./modules/*": "./src/modules/*.ts",
    "./openapi": "./src/openapi/openapi.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env tsx watch ./src/server.ts",
    "start": "pnpm with-env tsx ./src/server.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "with-env": "dotenv -e ../../.env.local --",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/db": "workspace:*",
    "@acme/validators": "workspace:*",
    "@auth/express": "^0.8.4",
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@stellar/stellar-sdk": "^13.0.0",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@trpc/server": "^11.0.0-rc.477",
    "@vitest/coverage-v8": "^2.1.8",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcrypt-edge": "^0.1.0",
    "cors": "^2.8.5",
    "cron": "^3.3.2",
    "express": "^4.21.2",
    "fastify": "^5.1.0",
    "fastify-metrics": "^12.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.7.2",
    "minio": "^8.0.3",
    "pino": "^9.5.0",
    "prom-client": "^15.1.3",
    "superjson": "2.2.1",
    "trpc-to-openapi": "^2.1.0",
    "tsyringe": "^4.8.0",
    "typedi": "^0.10.0",
    "vitest": "^2.1.8",
    "zod": "^3.23.8",
    "zod-openapi": "^4.1.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.7.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "prettier": "@acme/prettier-config"
}
